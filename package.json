    "helmet": "^8.1.0",
    "html-to-image": "^1.11.13",
    "joi": "^18.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.8.0",
    "natural": "^8.1.0",
    "openai": "^4.52.2",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
