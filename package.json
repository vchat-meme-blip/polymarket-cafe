{
  "name": "poly-cafe",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": "./dist/server/server/index.js",
  "main": "./dist/server/server/index.js",
  "types": "./dist/server/server/index.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:client": "vite --host --port 5173",
    "dev:server": "nodemon -q --watch server --ext ts,mts,js,mjs,json --exec \"node --import tsx server/index.ts\"",
    "build:client": "vite build",
    "clean:dist": "rimraf dist",
    "build:server": "tsc --project tsconfig.server.json --noEmit false --skipLibCheck true --pretty --noImplicitAny false --strictNullChecks false --strictFunctionTypes false --strictBindCallApply false --noImplicitThis false --noUnusedLocals false --noUnusedParameters false --noImplicitReturns false --noFallthroughCasesInSwitch false --outDir dist/server",
    "postbuild:server": "tsc-alias -p tsconfig.server.json",
    "build": "npm run clean:dist && npm run build:client && npm run build:server && npm run postbuild:server",
    "start": "NODE_ENV=production node dist/server/server/index.js",
    "start:pm2": "NODE_ENV=production pm2-runtime start ecosystem.config.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "reset-db": "tsx server/scripts/reset-db.ts"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.16.0",
    "@google/genai": "^1.16.0",
