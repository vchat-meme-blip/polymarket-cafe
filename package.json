{
  "name": "ai-dreams",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "exports": "./dist/server/index.js",
  "main": "./dist/server/index.js",
  "types": "./dist/server/index.d.ts",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:client": "vite --host --port 5173",
    "dev:server": "nodemon -q --watch server --ext ts,mts,js,mjs,json --exec \"node --import tsx server/index.ts\"",
    "build:client": "vite build",
    "clean:dist": "rimraf dist",
    "build:server": "tsc --project tsconfig.server.json --noEmit false --skipLibCheck true --pretty --noImplicitAny false --strictNullChecks false --strictFunctionTypes false --strictBindCallApply false --noImplicitThis false --noUnusedLocals false --noUnusedParameters false --noImplicitReturns false --noFallthroughCasesInSwitch false --outDir dist/server --rootDir .",
    "build:workers": "node scripts/build-workers.js && tsc --project tsconfig.worker.json --module esnext --esModuleInterop true --outDir dist/server/workers --noEmit false --skipLibCheck true --pretty --noImplicitAny false --strictNullChecks false --strictFunctionTypes false --strictBindCallApply false --noImplicitThis false --noUnusedLocals false --noUnusedParameters false --noImplicitReturns false --noFallthroughCasesInSwitch false",
    "postbuild:server": "tsc-alias -p tsconfig.server.json",
    "postbuild:workers": "tsc-alias -p tsconfig.worker.json",
    "build": "npm run clean:dist && npm run build:client && npm run build:server && npm run build:workers && npm run postbuild:server && npm run postbuild:workers",
    "start": "cross-env NODE_ENV=production node dist/server/index.js",
    "start:pm2": "cross-env NODE_ENV=production pm2-runtime start ecosystem.config.mjs",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.20.1",
    "@google/genai": "^1.27.0",
    "@mendable/firecrawl-js": "^4.4.1",
    "@pixiv/three-vrm": "^3.4.3",
    "@pixiv/three-vrm-animation": "^3.4.3",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@react-three/postprocessing": "^2.19.1",
    "@types/joi": "^17.2.3",
    "@types/mongoose": "^5.11.97",
    "@types/natural": "^5.1.5",
    "@types/node": "^22.18.12",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@types/socket.io": "^3.0.2",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.2",
    "classnames": "^2.5.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "eventemitter3": "^5.0.1",
    "express": "^4.21.2",
    "fs-extra": "^11.3.2",
    "html-to-image": "^1.11.13",
    "joi": "^18.0.1",
    "lodash": "^4.17.21",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "natural": "^8.1.0",
    "openai": "^4.104.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.164.1",
    "tsx": "^4.20.6",
    "twilio": "^5.10.3",
    "ws": "^8.18.3",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.24",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.20",
    "@types/mongodb": "^4.0.7",
    "@types/three": "^0.164.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.24",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-tsconfig-paths": "^4.3.2"
  }
}